<h1>Add a New Review for <%=@mural.name%></h1>

<%=image_tag @mural.photo_url%>

<%= form_for [@mural, @review] do |f| %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <br /><br />

  <%= f.label :review %>
  <%= f.text_field :review %>
  <br /><br />

  <%= f.submit "Add Review" %>

<% end %>
