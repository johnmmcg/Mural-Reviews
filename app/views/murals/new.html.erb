<h1>Add a New Mural</h1>

<%= form_for @mural do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br /><br />

  <%= f.label :location %>
  <%= f.text_field :location %>
  <br /><br />

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br /><br />

  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <br /><br />

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>
  <br /><br />

  <%= f.submit "Add Mural" %>

<% end %>
