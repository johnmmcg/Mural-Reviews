
<h2> Reviews </h2>
<% @reviews.each do |review| %>

  <% if current_user && current_user.admin? %>
    <%= button_to 'Delete Review', review_path(review), method: :delete %>
  <% end %>
<%end%>

<% if current_user && current_user.admin? %>
  <%= button_to 'Delete Mural', mural_path(@mural), method: :delete %>
<% end %>

<%= link_to "Add Review", new_mural_review_path(@mural)%>

<div id="app">

</div>
